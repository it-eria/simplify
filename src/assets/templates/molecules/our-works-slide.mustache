{{#mol-our-works-slide}}

    {{#image}}
        <div class="our-works__slider__item d-flex align-items-center  flex-lg-row flex-column-reverse">
            <div class="col-12 col-lg-4 description">
                <div class="info text-right">
                    {{#title}} <h1>{{title}}</h1>{{/title}}
                    {{#url}}<a href="{{{url}}}">{{{url}}}</a>{{/url}}
                </div>

            </div>
            <div class="col-12 col-lg-8 mask-container">
                <img data-name="{{data-name}}" src="{{{image}}}" alt="img" class="img">
            </div>
        </div>
    {{/image}}
{{/mol-our-works-slide}}